{
  "name": "thin-hook",
  "version": "0.3.3",
  "description": "Thin Hook Preprocessor",
  "main": "hook.js",
  "scripts": {
    "test": "wct",
    "build": "gulp",
    "demo": "run-p -l demoServer errorReportService",
    "upload": "run-p -l demoServer cacheBundleUploadService",
    "cache-bundle": "run-p -r -l demoServer cacheBundleUploadService cacheBundleGeneration",
    "polyserve": "polyserve -p 8080 -H 0.0.0.0 --compile never --proxy-path /components/thin-hook/demo/errorReport.json --proxy-target http://localhost:8081/errorReport.json",
    "demoServer": "node demo/demoServer.js -p 8080",
    "errorReportService": "node demo/errorReportService.js",
    "cacheBundleUploadService": "node demo/cacheBundleUploadService.js",
    "cacheBundleGeneration": "node demo/cacheBundleGeneration.js",
    "test:attack": "run-p -r -l demoServer cacheBundleUploadService puppeteerAttackTest",
    "puppeteerAttackTest": "node test/puppeteerAttackTest.js"
  },
  "bin": {
    "hook": "./hook-cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t2ym/thin-hook.git"
  },
