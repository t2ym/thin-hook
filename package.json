    "convert-source-map": "^1.5.0",
    "escodegen": "github:t2ym/escodegen#master",
    "espree": "github:t2ym/espree#master",
    "he": "^1.1.1",
    "htmlparser2": "3.9.2",
    "import-maps": "github:t2ym/import-maps#browserify",
    "pako": "^1.0.10",
